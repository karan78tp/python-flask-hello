{
  "id": "/flask-app123",
  "acceptedResourceRoles": ["slave_public"],
  "container": {
    "type": "DOCKER",
    "docker": {
      "image": "mesosphere/python-flask-hello:latest"
    },
    "portMappings": [
      {
        "containerPort":80,
        "hostPort":0,
        "protocol": "tcp",
        "name": "flask-app123"
      }
    ]
  },
  "cpus": 0.1,
  "instances": 1,
  "labels": { "VIP_0": "/dcos-101/app22:10000" },
  "mem": 128,
  "networks": [
    {
      "mode": "container/bridge",
      "labels": {}
    }
  ],
  "upgradeStrategy": {
    "maximumOverCapacity": 1,
    "minimumHealthCapacity": 1
  }
}
